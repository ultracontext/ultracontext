ULTRACONTEXT_API_KEY=uc_live_xxx
ULTRACONTEXT_BASE_URL=https://api.ultracontext.ai
REDIS_URL=redis://127.0.0.1:6379
INGESTOR_ENGINEER_ID=engineer-001
INGESTOR_HOST=
INGESTOR_POLL_MS=1500
INGESTOR_LOG_LEVEL=info
INGESTOR_LOG_APPENDS=true
INGESTOR_UI_MODE=auto
INGESTOR_UI_REFRESH_MS=1200
INGESTOR_UI_RECENT_LIMIT=10
INGESTOR_SOUND_ENABLED=true
INGESTOR_STARTUP_SOUND_ENABLED=true
INGESTOR_CONTEXT_SOUND_ENABLED=true
INGESTOR_STARTUP_GREETING_FILE=./assets/sounds/hello_mf.mp3
INGESTOR_CONTEXT_SOUND_FILE=./assets/sounds/quack.mp3
INGESTOR_CONFIG_FILE=~/.ultracontext/ingestor.config.json
INGESTOR_BOOTSTRAP_MODE=prompt
INGESTOR_BOOTSTRAP_RESET=false

# Source toggles
INGEST_CODEX=true
INGEST_CLAUDE=true
INGEST_OPENCLAW=true

# Glob patterns
CODEX_GLOB=~/.codex/sessions/**/*.jsonl
CLAUDE_GLOB=~/.claude/projects/**/*.jsonl
OPENCLAW_GLOB=~/.openclaw/agents/*/sessions/**/*.jsonl
CLAUDE_INCLUDE_SUBAGENTS=false

# Resume workflow (pull sessions from UltraContext)
RESUME_CONTEXT_LIMIT=200
RESUME_SOURCE_FILTER=all
RESUME_OPEN_TAB=true
RESUME_SUMMARY_TAIL=14
RESUME_OUTPUT_DIR=~/.codex/resume
